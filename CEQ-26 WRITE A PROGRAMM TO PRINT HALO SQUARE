#include <stdio.h>

int main() {
    char symbol;
    printf("Enter a symbol: ");
    scanf("%c", &symbol);
    
    int i, j;
    for (i = 1; i <= 5; i++) {
        for (j = 1; j <= 5; j++) {
            if (i == 1 || i == 5 || j == 1 || j == 5) {
                printf("%c ", symbol);
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    
    return 0;
}
